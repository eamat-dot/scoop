{
    "homepage": "https://www.centbrowser.com/",
    "version": "3.5.3.39",
    "persist": [
        "User Data"
    ],
    "architecture": {
        "64bit": {
            "url": "http://static.centbrowser.com/installer_64/centbrowser_3.5.3.39_x64_portable.exe",
            "hash": ""
        }
    },
    "bin": [
          "chrome.exe",
          [
               "chrome.exe"
               "centbrowser",
               "centbrowser"
          ],
    ]
    "installer": {
        "args": [
            "-s",
            "-d\"$dir\""
        ]
    },
    "shortcuts": [
        [
            "chrome.exe",
            "CentBrowser"
        ]
    ],
    "checkver": {
        "re": "<p>Version: ([\\d.]+)",
        "url": "https://www.centbrowser.com/index.html"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "http://static.centbrowser.com/installer_32/centbrowser_$version_portable.exe"
            }
        }
    }
}
